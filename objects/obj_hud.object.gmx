            <string>// Draw the player's stamina bar
if (instance_exists(obj_player)) {
    BarIndent = 50;
    BarWidth = view_wport[0]/5 - (BarIndent * 2);
    BarHeight = 5;
    BarTop = BarIndent;
    
    // stamina_fraction = obj_player.endurance / obj_player.stamina;
    
    draw_set_colour(c_green);
    draw_set_alpha(0.7);
    draw_rectangle(BarIndent, BarTop, BarIndent + BarWidth, BarTop + BarHeight, true);
    draw_rectangle(BarIndent, BarTop, BarIndent + (BarWidth * obj_player.stamina_fraction), BarTop + BarHeight, false);
    draw_set_alpha(1.0);
    
    draw_set_colour(c_red);
    draw_set_alpha(0.7);
    draw_rectangle(BarIndent, BarTop+10, BarIndent + BarWidth, BarTop+10 + BarHeight, true);
    draw_rectangle(BarIndent, BarTop+10, BarIndent + (BarWidth * obj_player.hp_fraction), BarTop+10 + BarHeight, false);
    draw_set_alpha(1.0);
    
    // Debugging info
    
    draw_text(10, 80, "stamina_fraction: " + string(obj_player.stamina_fraction));
    draw_text(10, 110, "cur_stamina: " + string(obj_player.cur_stamina));
    draw_text(10, 140, "dashing: " + string(obj_player.dashing));
    draw_text(10, 170, "cur_hp: " + string(obj_player.cur_hp));
}

if (instance_exists(obj_boss)) {
    draw_text(10, 200, "boss cur_hp: " + string(obj_boss.cur_hp));
    draw_text(10, 230, "boss image_index: " + string(obj_boss.image_index));
}

